{
  "name": "@conductor/secrets",
  "version": "1.0.0",
  "description": "Secrets management for Conductor with Doppler, Google Secret Manager, and Cloudflare support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@dopplerhq/node-sdk": "^1.2.0",
    "@google-cloud/secret-manager": "^5.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@conductor/core": "*"
  }
}
