{
  "name": "@conductor/cli",
  "version": "1.0.0",
  "description": "CLI for Conductor multi-LLM orchestration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "conductor": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@conductor/core": "*",
    "@conductor/e2b-runner": "*",
    "@conductor/state": "*",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "files": [
    "dist"
  ]
}
