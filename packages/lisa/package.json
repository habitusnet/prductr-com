{
  "name": "@prductr/lisa",
  "version": "1.0.0",
  "description": "Archaeological rescue & migration agent for abandoned projects",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lisa": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "project-rescue",
    "migration",
    "planning",
    "roadmap",
    "archaeological-analysis"
  ],
  "author": "prductr",
  "license": "MIT"
}
